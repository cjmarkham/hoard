#!/usr/bin/env php
<?php

include dirname(__DIR__) . '/bootstrap.php';
use Console\Application;

$application = new Application(
    'Hoard CLI',
    Hoard\Application::$version,
    $app
);
$application->addCommands(array(
    'Reports\ListAll',
    'Reports\Run',
    'Generate\Events',
));
$application->run();
